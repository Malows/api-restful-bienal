doctype html
html
  head
    include ./includes/estilos.jade
    title= title
  body
    div.brand Bienal Arte Joven
    div.address-bar Rectorado, UNL, siempre cerca de tu coraz√≥n
    div.container.box
    
      each val, index in messages
        div.alert.alert-danger(role="alert")= val
      div.col-xs-offset-3.col-xs-6  
        form(method="post", action="/signin")
          div.form-group
            label Username:
            input.form-control(type="text", name="username")
            label Password:
            input.form-control(type="password", name="password")
            hr
            button.btn.btn-info(type="submit", name="Sign in") Ingresar
      
    
